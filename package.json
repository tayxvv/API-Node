{
  "name": "nodetype",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npx ts-node-dev --inspect --transpile-only --ignore-watch node_modules --respawn src/server.ts",
    "typeorm": "npx ts-node-dev ./node_modules/typeorm/cli",
    "migration:create": "npx ts-node-dev ./node_modules/.bin/typeorm migration:create",
    "migration:up": "npx ts-node-dev ./node_modules/.bin/typeorm migration:run -d ormconfig.ts",
    "migration:down": "npx ts-node-dev ./node_modules/.bin/typeorm migration:revert -d ormconfig.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/multer": "^1.4.7",
    "csv-parse": "^5.3.8",
    "csv-parser": "^3.0.0",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "mutter": "^1.0.1",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.6.2",
    "typeorm": "^0.3.15",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.38.0",
    "typescript": "^5.0.4"
  }
}
