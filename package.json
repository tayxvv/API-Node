{
  "name": "nodetype",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npx ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn src/shared/infra/http/server.ts",
    "typeorm": "npx ts-node-dev ./node_modules/typeorm/cli",
    "migration:create": "npx ts-node-dev ./node_modules/.bin/typeorm migration:create",
    "migration:up": "npx ts-node-dev ./node_modules/.bin/typeorm migration:run -d ormconfig.ts",
    "migration:down": "npx ts-node-dev ./node_modules/.bin/typeorm migration:revert -d ormconfig.ts",
    "test": "NODE_DEV=test jest --runInBand --detectOpenHandles",
    "seed:admin": "ts-node-dev src/shared/infra/typeorm/seed/admin.ts"
  },
  "keywords": [],
